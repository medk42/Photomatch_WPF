<UserControl x:Class="Photomatch_ProofOfConcept_WPF.WPF.View.ModelView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Photomatch_ProofOfConcept_WPF.WPF.View"
             xmlns:behavior="clr-namespace:Photomatch_ProofOfConcept_WPF.WPF.Helper"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             behavior:ControlProperties.MouseHandler="{Binding MouseHandler}"
             ClipToBounds="True" Background="White" Content="{Binding ModelContent}">
</UserControl>
